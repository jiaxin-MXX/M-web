{{each data}}
<li data-to="xiangqing" data-spuId="{{$value.id}}" class='one find'>
        <div class='imgs'>
            <img src='http://localhost:4000/{{$value.firstname}}/{{$value.tupian[0]}}' alt='商品图片'>
        </div>
        <div class='jianjie'>
            <h3>{{$value.title}}</h3>
        </div>
        <div class='prices'>
            <span>￥</span>
            <span>{{$value.shoujia}}</span>
        </div>
</li>
{{/each}}
